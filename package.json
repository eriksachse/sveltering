{
  "name": "sveltering",
  "version": "0.0.6",
  "author": "Erik Sachse <erik@napoleon.services> (https://napoleon.services/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/eriksachse/sveltering"
  },
  "homepage": "ttps://github.com/eriksachse/sveltering",
  "main": "index.js",
  "module": "index.js",
  "svelte": "index.js",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "next",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0"
  },
  "type": "module"
}
